{
    "name": "savingssimulation/amazon",
    "description": "A static site generator for Amazon savings simulation.",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.5",
        "twig/twig": "^3.4",
        "symfony/yaml": "^6.1",
        "erusev/parsedown": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
